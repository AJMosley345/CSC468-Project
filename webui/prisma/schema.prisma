generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Courses {
  course_id         Int                 @id @default(autoincrement()) @unique
  course_number     String?             @db.VarChar(250) 
  course_name       String?             @db.VarChar(250)
  taught_by         Professor[]    
  taken_by          Student[]           
}

model Professor {
  professor_id      Int                 @id @default(autoincrement()) @unique
  username          String?             @db.VarChar(250) @unique
  pass              String?             @db.VarChar(250) @unique
  fullName          String?             @db.VarChar(250)
  courses_taught    Courses[]
}

model Student {
  student_id      Int               @id @default(autoincrement()) @unique
  username        String?           @db.VarChar(250) @unique
  pass            String?           @db.VarChar(250) @unique
  fullName        String?           @db.VarChar(250)
  courses_taken  Courses[]
}

model meetings {
  meeting_id Int @id @default(autoincrement())
}