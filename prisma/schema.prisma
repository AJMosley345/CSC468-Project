generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model courses {
  course_id       Int               @id @default(autoincrement())
  course_number   String?           @db.VarChar(250)
  course_name     String?           @db.VarChar(250)
  student_courses student_courses[]
}

model professors {
  professor_id Int     @id @default(autoincrement())
  username     String? @db.VarChar(250)
  pass         String? @db.VarChar(250)
  fullName     String? @db.VarChar(250)
}

model students {
  student_id      Int               @id @default(autoincrement())
  username        String?           @db.VarChar(250)
  pass            String?           @db.VarChar(250)
  fullName        String?           @db.VarChar(250)
  student_courses student_courses[]
}

model student_courses {
  id Int @id @default(autoincrement())
  course_id  Int
  student_id Int
  courses    courses  @relation(fields: [course_id], references: [course_id], onDelete: Cascade, onUpdate: NoAction, map: "student_courses_ibfk_1")
  students   students @relation(fields: [student_id], references: [student_id], onDelete: Cascade, onUpdate: NoAction, map: "student_courses_ibfk_2")
}

model Meeting {
  meeting_id   Int     @id @default(autoincrement())
  meeting_name String? @db.VarChar(250)
}
